{
  "compilerOptions": {
    "module": "ES6",
    "lib": [
      "ES6",
      "ES2020",
    ],
    "target": "ES6",
    "allowJs": true,
    "checkJs": false,
    "moduleResolution": "Classic",
    "noEmit": true,
  },
  "include": [
    "../../sdk",
    "**/*.js"
  ],
  "exclude": [
    "node_modules",
    "**/node_modules/*"
  ],
}
